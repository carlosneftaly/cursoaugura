datos<-read.csv('DBA.csv',header=T,sep=';')
attach(datos)
datos
boxplot(Esporas~Tratamiento+Bloque,las=2)
stripchart(Esporas~Tratamiento+Bloque,vert=T,pch=21,col='red',add=T)
hist(Esporas,prob=T)
lines(density(Esporas))
modelo1<-lm(Esporas~Tratamiento+Bloque)
modelo2<-aov(Esporas~Tratamiento+Bloque)
summary(modelo1)
summary(modelo2)
shapiro.test(modelo1$residuals)
par(mfrow=c(2,2))
plot(modelo1)
par(mfrow=c(1,1))
residuales<-resid(modelo1)
boxplot(residuales~Tratamiento, data=datos, ylim=c(-6,6), ylab="Residuales", xlab="Tratamiento", main="Homogeneidad de residuales")
boxplot(residuales~Tratamiento, data=datos, ylab="Residuales", xlab="Tratamiento", main="Homogeneidad de residuales")
stripplot(residuales~Tratamiento, data=datos, ylab="Residuales", xlab="Tratamiento", main="Homogeneidad de residuales")
library(lattice)
library(car)
require(multcomp)
require(agricolae)
stripplot(residuales~Tratamiento, data=datos, ylab="Residuales", xlab="Tratamiento", main="Homogeneidad de residuales")
leveneTest(modelo1)
leveneTest(Esporas)~interaction(Tratamiento,Bloque))
leveneTest(Esporas~interaction(Tratamiento,Bloque))
bartlett.test(Esporas~Tratamiento+Bloque)
bartlett.test(Esporas~interaction(Tratamiento,Bloque))
modelo1<-lm(log(Esporas)~Tratamiento+Bloque)
shapiro.test(modelo1$residuals)
leveneTest(log(Esporas)~interaction(Tratamiento,Bloque))
anova(modelo1)
summary(glht(modelo1,linfct=mcp(Tratamiento="Dunnett")))
out<-HSD.test(modelo1,"Tratamiento", group=TRUE)
out
